    <rule id="8e68e487-7a76-9cdb-dbb4-2af0ac48d773" name="Wrastler" hidden="false" book="Forces of Hordes - Minions" page="57">
      <description>While knocked down, this model can make</description>
      <modifiers/>
    </rule>
