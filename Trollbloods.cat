    <rule id="1c1beb4a-e34e-59df-797c-ab79b68069b2" name="Minion (Circle, Legion, Skorne, and Trollbloods)" hidden="false" book="Forces of Hordes - Minions" page="32">
      <description>This model will work for Cirlce, Legion, Skorne, and Trollbloods.</description>
      <modifiers/>
