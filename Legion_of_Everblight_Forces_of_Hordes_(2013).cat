    <entry id="5a5d3016-d53b-97c5-cda0-b88adfccb070" name="Thagrosh, The Messiah" points="-3.0" categoryId="5761726c6f636b23232344415441232323" type="model" minSelections="0" maxSelections="-1" minInForce="0" maxInForce="-1" minInRoster="0" maxInRoster="-1" minPoints="0.0" maxPoints="-1.0" collective="false" hidden="false" book="Forces of Hordes - Legion of Everblight" page="48">
