      <modifiers/>
    </rule>
    <rule id="6cb14c66-099c-e2b5-a97b-3fb4cd2d4d3c" name="Regeneration (d3)" hidden="false" book="Forces of Hordes - Legion of Everblight" page="68">
      <description>This model can be forced to heal d3 damage points once per activation. This model cannot use Regeneration during an activation it runs.</description>
      <modifiers/>
    </rule>
    <rule id="c09e2a0e-111d-aa4b-e9a3-210f44fef604" name="Resourceful" hidden="false" book="Forces of Hordes - Legion of Everblight" page="32">
      <description>This model can uplkeep spells on models in its battlegroup without spending fury.</description>
      <modifiers/>
