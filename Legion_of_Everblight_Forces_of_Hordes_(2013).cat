      <modifiers/>
    </rule>
    <rule id="3d0addb5-0ec0-3ef4-9633-d1c9fcc6fb5c" name="Immunity: Corrosion" hidden="false" book="Hordes Primal MK II" page="35">
      <description>This model does not suffer corrosion damage and is immune to the Corrosion continuous effect.</description>
      <modifiers/>
    </rule>
    <rule id="905bf3a5-2d9a-93bd-3d37-b7fc3fc84e76" name="Immunity: Electricity" hidden="false" book="Hordes Primal MK II" page="35">
      <description>This model does not suffer electricla damage.</description>
      <modifiers/>
    </rule>
    <rule id="707250e6-91df-89b6-4035-34c9a11f1925" name="Immunity: Fire" hidden="false" book="Hordes Primal MK II" page="35">
