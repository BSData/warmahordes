      <modifiers/>
    </rule>
    <rule id="0f32-da1b-7e20-2aa9" name="Tactics: Ranked Attacks" hidden="false" book="Forces of Warmachine - Convergence of Cyriss" page="81">
      <modifiers/>
    </rule>
    <rule id="68b0-5274-700a-a345" name="Bodge (* Action)" hidden="false" book="Forces of Warmachine - Convergence of Cyriss" page="82">
      <description>This model performs minor repairs to one damaged friendly Faction warjack with which it is B2B. Remove 1 damage point from the warjack.</description>
      <modifiers/>
    </rule>
    <rule id="6462-e8a8-c9c1-1b1f" name="Flare" hidden="false" book="Forces of Warmachine - Convergence of Cyriss" page="84">
      <description>Models hit by this attack suffer -2 DEF for one round.</description>
      <modifiers/>
    </rule>
    <rule id="87be-2105-ec17-c347" name="Gun Platform" hidden="false" book="Forces of Warmachine - Convergence of Cyriss" page="90">
      <description>This model can make ranged attacks even while in melee.	</description>
      <modifiers/>
