    <rule id="4cdb58a9-89c7-e86d-6b6b-554a6cee3d42" name="Damage Type: Cold" hidden="false" book="Warmachine Prime mkII" page="35">
      <description>This weapon causes cold damage (p.68).</description>
      <modifiers/>
    </rule>
    <rule id="a97181c0-e9ea-5063-8069-ecf1bb286402" name="Damage Type: Corrosion" hidden="false" book="Warmachine Prime mkII" page="35">
      <description>This weapon causes corrosion damage (p. 68).</description>
      <modifiers/>
    </rule>
    <rule id="f4dce6be-217f-0c24-6f50-d00d547dd53c" name="Damage Type: Electricitiy" hidden="false" book="Warmachine Prime mkII" page="35">
      <description>This weapon causes electrical damage (p. 68).</description>
      <modifiers/>
    </rule>
    <rule id="d50f482e-11ab-3628-b838-6458b5e339d2" name="Damage Type: Fire" hidden="false" book="Warmachine Prime mkII" page="0">
      <description>This weapon causes fire damage (p. 68).</description>
      <modifiers/>
    </rule>
    <rule id="cbee9210-d756-6712-f628-71c930fafa24" name="Death Blast" hidden="false" book="Forces of Warmachine: Protectorate of Menoth" page="117">
      <description>When this model is disabled, center a 5&quot; AOE on it, then remove this model from play. Models in the AOE suffer a POW 14 blast damage roll.</description>
