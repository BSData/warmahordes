      <modifiers/>
    </rule>
    <rule id="756ae4a2-7de8-4fbb-9db3-35a07ba9b50e" name="Advisor" hidden="false" book="Mercenaries - Forces of Warmachine" page="70">
      <description>While B2B with this model, friendly warcastes and solos gain +1 CMD.</description>
      <modifiers/>
    </rule>
    <rule id="19580991-8087-4f1c-bcce-f011aa387323" name="Aegis" hidden="false" book="Forces of Warmachine: Protectorate of Menoth" page="56">
