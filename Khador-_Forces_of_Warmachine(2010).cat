      <modifiers/>
    </rule>
    <rule id="3300c3df-350e-9d5b-094a-ffae33419f0d" name="Sniper" hidden="false" book="Mercenaries - Forces of Warmachine" page="64">
      <description>When damaging a warjack or warbeast with a ranged attack, choose which column or branch suffers damage. Instead of rolling damage on a ranged attack, this model can inflict 1 damage point. A model that participates in a combined ranged attack loses Sniper until the attack is resolved.</description>
      <modifiers/>
    </rule>
    <rule id="7e5e7684-d2a1-b0a8-7a78-2ad80d661910" name="Soul Collector" hidden="false" book="Colossal" page="128">
